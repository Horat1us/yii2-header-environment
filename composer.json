{
  "name": "horat1us/yii2-header-environment",
  "description": "Loading environment variables from HTTP Headers (for testing purposes)",
  "type": "library",
  "require": {
    "php": ">=7.1",
    "yiisoft/yii2": "2.0.15.1"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.1",
    "squizlabs/php_codesniffer": "^3.2"
  },
  "license": "MIT",
  "authors": [
    {
      "name": "Alexander Letnikow",
      "email": "reclamme@gmail.com"
    }
  ],
  "minimum-stability": "stable",
  "autoload": {
    "psr-4": {
      "Horat1us\\HeaderEnvironment\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Horat1us\\HeaderEnvironment\\Tests\\": "tests/"
    }
  },
  "config": {
    "process-timeout": 1800,
    "sort-packages": true,
    "platform": {
      "php": "7.1"
    }
  },
  "scripts": {
    "lint": "./vendor/bin/phpcs --standard=PSR2 ./src ./tests",
    "test": "./vendor/bin/phpunit"
  },
  "suggest": {
    "horat1us/environment-config": "Deal with multiple environment configs with prefixes"
  }
}
